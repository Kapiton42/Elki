%h1 Listing time_of_arrs

%table
  %thead
    %tr
      %th Timeofarrival
      %th Station
      %th Graphik
      %th
      %th
      %th

  %tbody
    - @time_of_arrs.each do |time_of_arr|
      %tr
        %td= time_of_arr.timeOfArrival
        %td= time_of_arr.station.name
        %td= "Begin: " + time_of_arr.graphik.station_begin.name + ", End:" + time_of_arr.graphik.station_end.name
        %td= link_to 'Show', time_of_arr
        %td= link_to 'Edit', edit_time_of_arr_path(time_of_arr)
        %td= link_to 'Destroy', time_of_arr, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Time of arr', new_time_of_arr_path
